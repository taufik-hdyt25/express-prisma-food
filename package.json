{
  "name": "PrismaExpress",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "migrate": "prisma migrate deploy",
    "start": "if [ -z \"$(prisma migrate status --schema=./prisma/schema.prisma --json | jq -r .pendingMigrationCount)\" ]; then echo 'No pending migrations'; else echo 'Pending migrations found' && prisma migrate deploy; fi && nodemon"
  },
  "dependencies": {
    "@cloudinary/url-gen": "^1.20.0",
    "@prisma/client": "^5.16.2",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.16.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.10",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
